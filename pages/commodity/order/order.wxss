/* pages/order/order.wxss */
.pageTitle {
    width: 100%;
    height: 100rpx;
    align-items: center;
    justify-content: center;
    font-size: 40rpx;
    color: #fff;
    background-color: #74c26c;
    padding: 10px; 
    box-sizing: border-box;
}
.address_border {
    padding-bottom: 16px;
    background: #fff url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAIAAAAAKBAMAAACOO0tGAAAABGdBT…DXLwKT58CuX2cDTIEjdgOcYfJJ2A3Qgsmz4/CBwWgQUiMIAXzCOFELLk/nAAAAAElFTkSuQmCC) -7px bottom repeat-x;
    background-size: 64px 5px;
}
.address_defalut text {
    word-wrap: break-word;
    word-break: break-all;
    font-size: 14px;
}
.order_shopBar {
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    font-weight: 700;
}

.order_shopBar_jd {
    display: inline-block;
    vertical-align: middle;
    margin-right: 5px;
    width: 16px;
    height: 15px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAeBAMAAAC/JAEaAAAABGdBT…6T6vTdoPDgBgaUFANSiAEFthswIIUpt1jFApBTUEKdQgAA8OVkVWjqwfoAAAAASUVORK5CYII=) no-repeat;
    background-size: 16px auto;
}
.shipping {
    padding: 10px;
}
.hproduct {
    position: relative;
    min-height: 75px;
    padding: 0;
    font-size: 12px;
    padding-top: 15px;
}
 .hproduct .photo {
    width: 75px;
    height: 75px;
    position: absolute;
    top: 15px;
    left: 0;
}
image {
    border: 0 none;
    vertical-align: top;
}
 .fn {
    color: #333;
    line-height: 20px;
    margin-bottom: 5px;
    font-size: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
 .fn {
    padding-left: 85px;
}
.mod_sign_tip.bor.color_green {
    color: #18c461;
}

.mod_sign_tip.bor {
    color: #e4393c;
    background: #fff;
}
.mod_sign_tip {
    display: inline-block;
    background: #e4393c;
    position: relative;
    color: #fff;
    border-radius: 2px;
    min-width: 16px;
    font-size: 10px;
    line-height: 15px;
    vertical-align: middle;
    margin-top: -2px;
    margin-right: 5px;
    text-align: center;
}
 .sku_coll {
    margin-bottom: 6px;
    font-size: 12px;
    color: #999;
    line-height: 1;
}

.sku_coll {
    padding-left: 85px;
}
.sku_price {
    padding-left: 85px;
    height: 30px;
    line-height: 30px;
    color: #e93b3d;
    font-size: 10px;
}
.sku {
    height: 30px;
    margin-top: -30px;
    text-align: right;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.sku_num {
    line-height: 30px;
    color: #999;
    font-size: 12px;
}
.order_info .order_info_list view {
    padding: 10px 20px 10px 0;
    position: relative;
}
.shipping_content {
    text-align: right;
}
.shipping_content .type_justify {
    overflow: hidden;
    margin: 0;
    color: #999;
}
.order_info_guarantee {
    position: relative;
    padding: 12px 0;
}
.shipping_content .type_justify .left {
    float: left;
}
.order_info_tips {
    margin: 6px 0;
    padding-left: 85px;
    overflow: hidden;
}
.order_info_tips view {
    float: left;
    font-size: 12px;
    color: #999;
    margin-right: 5px;
}
.order_info_tips view .icon_optional {
    display: inline-block;
    vertical-align: middle;
    width: 18px;
    height: 18px;
    margin: -2px 2px 0 0;
}
.order_info .order_info_guarantee {
    position: relative;
    padding: 12px 0;
}
.buy_checkout view text.disabled {
    color: #999;
}
.order_info_guarantee_title {
    margin-bottom: 3px;
    font-size: 16px;
    font-weight: 400;
    color: #333;
}
.order_info_guarantee_content {
    width: 62%;
    font-size: 12px;
    color: #999;
}
.address {
    width: 80px;
    height: 30px;
    font-size: 14px;
    line-height: 30px;
    background: #e4393c;
    color: #fff;
    margin: 10px auto;
    margin-top: 10px;
    text-align: center;
    border-radius: 5px;
}
.order_info .order_info_guarantee .order_info_guarantee_content .info {
    display: inline-block;
    margin: -2px 0 0 5px;
    width: 12px;
    height: 12px;
    vertical-align: middle;
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAABGdBT…2CMPei+wfdv+jhgRFe6OGJEd7A+FADxkcSLD4w4gs5PgEv+xzk6eKJOwAAAABJRU5ErkJggg==);
    background-size: 12px;
}
.order_info_guarantee_price {
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -20px;
    padding-right: 25px;
    height: 45px;
    line-height: 45px;
    color: #e93b3d;
    font-size: 12px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAABGdBT…9dPbp56Pahuwfdvej+QfcvenhghBd6eGKEN3p8YMQXcnwCAMmhLp/s1mG7AAAAAElFTkSuQmCC) no-repeat right 12px;
    background-size: 20px;
}
.title {
    display: inline-block;
    font-weight: 400;
    position: absolute;
    left: 10px;
    top: 11px;
    font-size: 16px;
    color: #333;
}
.content {
    position: relative;
    display: block;
    text-align: right;
    font-size: 12px;
    color: #666;
    padding: 13px 30px 13px 75px;
}
.buy_checkout view text {
    padding-right: 0;
}
.buy_checkout view text {
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    color: #e93b3d;
}

.buy_checkout view {
    overflow: hidden;
}

.buy_chart_item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.buy_checkout view.content {
    position: relative;
    display: block;
    text-align: right;
    font-size: 12px;
    color: #666;
    /* padding: 13px 30px 13px 75px; */
}

.buy_chart_item {
    font-family: arial;
}
.buy_chart_item_text {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    color: #333;
}
.buy_chart_item_price {
    color: #e93b3d;
}
.buy_chart {
    position: relative;
    background-color: #fff;
    padding: 10px;
    font-size: 14px;
}

.address_defalut {
    background: #fff;
    padding: 12px 10px;
    position: relative;
}

.price {
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 20px;
    text-align: right;
}
.price text {
    color: #e93b3d;
    font-weight: 400;
}
 .mod_btn {
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 16px;
    margin: 0 10px;
}
.mod_btn.bg_2 {
    background: #74c26c;
    color: #fff;
    margin-bottom: 50px;
}