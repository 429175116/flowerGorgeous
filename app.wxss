    background: #fff;
    position: relative;
    padding-bottom: 10px;
}

.address_new view {
    padding: 12px 10px 12px 75px;
    position: relative;
    box-sizing: border-box;
}

.address_new .tit {
    width: 65px;
    line-height: 1;
    position: absolute;
    top: 20px;
    left: 0;
    right: 0;
    margin: auto 0;
    padding: 0 10px;
    font-size: 14px;
    color: #999;
}

.address_new input {
    height: 20px;
    line-height: normal;
    border: 0 none;
